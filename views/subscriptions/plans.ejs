<% layout('layout') %>

<h1><%= title %></h1>

<form method="get" action="/subscriptions/plans">
  <div class="row">
    <label>Status: </label>
    <select name="status">
      <option value="created" selected="selected">CREATED</option>
      <option value="active">ACTIVE</option>
      <option value="inactive">INACTIVE</option>
    </select>
  </div>
  <div class="row">
    <button class="btn btn-info">Search</button>
  </div>
</form>

<% if (JSON.stringify(error) !== '{}') { %>
  <div class="row">
    <h2>API call Failed</h2>
    <pre><code><%= errorStr %></code></pre>
  </div>
<% } %>

<% if (data.plansStr) { %>
  <div class="row">
    <h2>Result</h2>
    <pre><code><%= data.plansStr %></code></pre>
  </div>
<% } %>

