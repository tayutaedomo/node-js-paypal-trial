<% layout('layout') %>

<h1><%= title %></h1>

<div class="row">
  <div class="col-sm-12">
    <form method="get" action="/notifications/webhook_events" class="form-inline">
      <div class="form-group form-group-sm">
        <label>Page Size</label>
        <input type="number" name="page_size" class="form-control">
      </div>
      <div class="form-group form-group-sm">
        <button type="submit" class="btn btn-info">Search</button>
      </div>
    </form>
  </div>
</div>

<div class="row">
  <div class="col-sm-12">
    <% if (JSON.stringify(error) !== '{}') { %>
      <label>API call Failed</label>
      <pre class="bg-danger"><code><%= errorStr %></code></pre>
    <% } else { %>
      <label>Result</label>
      <pre class="bg-success"><code><%= data.webhookEventsStr %></code></pre>
    <% } %>
  </div>
</div>

